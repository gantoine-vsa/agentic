{
	"name": "agentic",
	"version": "1.0.0",
	"description": "AI SDK Examples",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc -p .",
		"lint": "tsc --noEmit -p .",
		"format": "biome format --write",
		"array": "tsx --env-file=.env src/array/index.ts",
		"agent": "tsx --env-file=.env src/agent/index.ts",
		"enum": "tsx --env-file=.env src/enum/index.ts",
		"evaluator": "tsx --env-file=.env src/evaluator/index.ts",
		"loan": "tsx --env-file=.env src/loan/index.ts",
		"mcp:sse:server": "tsx --env-file=.env src/mcp/sse/server.ts",
		"mcp:sse:client": "tsx --env-file=.env src/mcp/sse/client.ts",
		"mcp:stdio:client": "tsx --env-file=.env src/mcp/stdio/client.ts"
	},
	"author": "Sumanth Chinthagunta",
	"license": "MIT",
	"packageManager": "pnpm@10.7.0",
	"engines": {
		"node": ">=20",
		"pnpm": ">=9"
	},
	"type": "module",
	"dependencies": {
		"@ai-sdk/azure": "^1.3.11",
		"@ai-sdk/openai": "1.3.10",
		"@ai-sdk/openai-compatible": "0.2.9",
		"@biomejs/biome": "1.9.4",
		"@modelcontextprotocol/sdk": "1.9.0",
		"ai": "4.3.5",
		"express": "5.1.0",
		"ollama-ai-provider": "1.2.0",
		"zod": "3.24.2"
	},
	"devDependencies": {
		"@types/express": "5.0.1",
		"@types/node": "22.14.1",
		"tsx": "4.19.3",
		"typescript": "5.8.3"
	}
}
